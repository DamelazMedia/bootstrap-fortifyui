{
    "name": "DamelazMedia/bootstrap-fortifyui",
    "description": "Fortify-driven Laravel UI replacement bootstrap5",
    "keywords": [
        "fortify-ui"
    ],
    "homepage": "https://github.com/DamelazMedia/bootstrap-fortifyui",
    "license": "MIT",
    "authors": [
        {
            "name": "Dennis coenraats",
            "email": "denniscoenraats@gmail.com",
            "homepage": "",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/contracts": "^8.0",
        "zacksmash/fortify-ui": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "DamelazMedia\\FortifyUIPreset\\Bootstrap5\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DamelazMedia\\FortifyUIPreset\\Bootstrap5\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DamelazMedia\\FortifyUIPreset\\Bootstrap5\\FortifyUIPresetServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
